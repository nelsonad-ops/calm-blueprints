            "classification":"runshell",
            "script":"#!\/bin\/bash\ncat > \/tmp\/set-hostnames.py <<EOF\n#!\/usr\/bin\/python\nmaster,slaves=\"@@{calm_array_private_ip_address}@@\".split(\",\",1)\nwith open(\"\/etc\/hosts\", \"a\") as hostsfile:\n  hostsfile.write(\"#KUBE-START\\n\")\n  hostsfile.write(\"{} master\\n\".format(master))\n  count=1\n  for slave in slaves.split(','):\n    hostsfile.write(\"{} slave{}\\n\".format(slave, str(count)))\n    count += 1\n  hostsfile.write(\"#KUBE-END\\n\")\nEOF\nsudo sed -i -e '\/#KUBE-START\/,\/#KUBE-END\/c\\' \/etc\/hosts\nsudo python \/tmp\/set-hostnames.py",
